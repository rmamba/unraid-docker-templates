<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Date>2019-04-08</Date>
  <Beta>False</Beta>
  <Category>Cloud:</Category>
  <Name>portainer</Name>
  <Project>https://github.com/docker/distribution</Project>
  <Changes>
- Portainer 1.20.2
  </Changes>
  <Overview>
    Install Portainer on unRAID
  </Overview>
  <Description>
    Use Portainer on unRAID
  </Description>
  <Registry>https://hub.docker.com/r/portainer/portainer/</Registry>
  <GitHub>https://github.com/portainer/portainer</GitHub>
  <Repository>portainer/portainer:1.20.2</Repository>
  <Support>https://github.com/portainer/portainer/issues</Support>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment />
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9000</HostPort>
        <ContainerPort>9000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/var/run/docker.sock</HostDir>
      <ContainerDir>/var/run/docker.sock</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/Portainer</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Network Default="bridge">bridge</Network>

  <Config Type="Port" Name="Portainer Port" Target="9000" Default="9000" Mode="tcp" Display="always-hide" Required="true"
       Description="This is Portainer's communication port that can be accessed from browser.">9000</Config>
</Container>
